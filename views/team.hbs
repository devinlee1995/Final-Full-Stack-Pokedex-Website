<h1>All Users</h1>
<ul>	
	{{#each team}}
	<li>
		{{name}}
	</li>
	<div id = "team">
		<img src = "{{src}}">
	</div>
	{{/each}}
</ul>

<h2> Enter a Pokemon</h2>
<form action="/team/{{user.id}}" method="POST">
	<input type ="text" name = "name" placeholder = "Pokemon Name">
	<input type="submit">
</form>

